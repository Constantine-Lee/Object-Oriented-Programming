${OBJECTDIR}/Lecturer_nomain.o: ${OBJECTDIR}/Lecturer.o Lecturer.cpp 
	${MKDIR} -p ${OBJECTDIR}
	@NMOUTPUT=`${NM} ${OBJECTDIR}/Lecturer.o`; \
	if (echo "$$NMOUTPUT" | ${GREP} '|main$$') || \
	   (echo "$$NMOUTPUT" | ${GREP} 'T main$$') || \
	   (echo "$$NMOUTPUT" | ${GREP} 'T _main$$'); \
	then  \
	    ${RM} "$@.d";\
	    $(COMPILE.cc) -O2 -Dmain=__nomain -MMD -MP -MF "$@.d" -o ${OBJECTDIR}/Lecturer_nomain.o Lecturer.cpp;\
	else  \
	    ${CP} ${OBJECTDIR}/Lecturer.o ${OBJECTDIR}/Lecturer_nomain.o;\
