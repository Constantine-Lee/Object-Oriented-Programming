${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/ct2_test_2.exe: ${OBJECTFILES}
	${MKDIR} -p ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}
	${LINK.cc} -o ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/ct2_test_2 ${OBJECTFILES} ${LDLIBSOPTIONS}

